{
  "name": "confly",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@7.12.0",
  "description": "",
  "author": "Ying Cai <i@caiying.me>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/yingca1",
  "homepage": "https://github.com/yingca1/confly#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yingca1/confly.git"
  },
  "bugs": "https://github.com/yingca1/confly/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "bin": {
    "confly": "dist/cli.cjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@antfu/utils": "^0.7.2",
    "@types/consul": "^0.40.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.14.1",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "bumpp": "^9.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-yml": "^1.5.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.1.2",
    "pnpm": "^7.27.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.28.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "consul": "^1.2.0",
    "isomorphic-git": "^1.21.0",
    "js-yaml": "^4.1.0",
    "yargs": "^17.7.1"
  }
}
